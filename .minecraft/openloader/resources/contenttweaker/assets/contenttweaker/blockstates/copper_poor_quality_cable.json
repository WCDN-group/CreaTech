{
  "multipart": [
      {   "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_post_ends" }},
      {   "when": { "north": false, "east": false, "south": false, "west": false },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_post" }
      },
      {   "when": { "north": true, "east": false, "south": false, "west": false },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_cap" }
      },
      {   "when": { "north": false, "east": true, "south": false, "west": false },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_cap", "y": 90 }
      },
      {   "when": { "north": false, "east": false, "south": true, "west": false },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_cap_alt" }
      },
      {   "when": { "north": false, "east": false, "south": false, "west": true },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_cap_alt", "y": 90 }
      },
      {   "when": { "north": true },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_side" }
      },
      {   "when": { "east": true },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_side", "y": 90 }
      },
      {   "when": { "south": true },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_side_alt" }
      },
      {   "when": { "west": true },
          "apply": { "model": "contenttweaker:block/copper_poor_quality_cable_side_alt", "y": 90 }
      }
  ]
}
